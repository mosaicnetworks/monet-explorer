	"scripts": {
		"migrate": "./scripts/pre-django.sh",
		"tx": "ts-node scripts/transfer",
		"lint": "tslint -p . -c ./tslint.json",
		"root80": "PORT=80 react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"clean": "rm -rf node_modules build server/src/db.sqlite3",
		"docker:build:react": "docker build --rm -f ./docker/react/Dockerfile -t monet-explorer-react:latest .",
		"docker:build": "docker build --rm -f ./docker/Dockerfile -t monet-explorer:latest .",
		"docker:run": "docker run --rm -it -p 80:80 monet-explorer:latest",
		"dev:django": "cd server/src && python3 manage.py runserver",
		"dev:react": "PORT=3002 react-scripts start",
		"superuser": "python3 server/src/manage.py createsuperuser",
		"cron": "yarn cron:val && yarn cron:blocks",
		"cron:run": "python3 server/src/manage.py crontab run",
		"cron:add": "python3 server/src/manage.py crontab add",
		"cron:remove": "python3 server/src/manage.py crontab remove",
		"cron:blocks": "python3 server/src/manage.py crontab run 788ce65b788f26ca6bccb4bf3fac2c39",
		"cron:val": "python3 server/src/manage.py crontab run 044d31e5e577365ff6562d32f7a5af3b",
		"populate": "yarn cron:val && yarn cron:blocks",
		"dev": "concurrently --kill-others \"yarn dev:django\" \"yarn dev:react\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
